mixin fixed-header(json)
  +b.HEADER.fixed-header.js-fixed-header&attributes(attributes)
    .container
      +e.content
        +image-svg('logo-branded').__logo
        nav
          +list(getData(json))._sm._bold._gray.__list
        +e.buttons.hide-lg
          +button._xs._dark._app-store
            +image-svg('app-store')
          +button._xs._dark._google-play
            +image-svg('google-play')
